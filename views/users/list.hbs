<h3>
  <a class="btn btn-secondary" href="/Users">
    <i class="fa fa-plus"></i> Create New</a> Contact List
</h3>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Full Name</th>
      <th>Email</th>
      <th>Mobile</th>
      <th>Pet Name</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    {{#each list}}
    <tr>
      <td>{{this.fullName}}</td>
      <td>{{this.email}}</td>
      <td>{{this.mobile}}</td>
      <td>{{this.petname}}</td>
      <td>
        <a href="/Users/{{this._id}}"><i class="fa fa-pencil fa-lg" aria-hidden="true"></i></a>
        <a href="/Users/delete/{{this._id}}" onclick="return confirm('Are you sure to delete this record ?');"><i
            class="fa fa-trash fa-lg" aria-hidden="true"></i></a>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>